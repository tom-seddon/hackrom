	LSR	A
	TAX
	LDA	psyntab,X
	TAX
	LDY	#$FF
	JSR	indent2
helploop
nh
	INY
	LDA	cpflag
	CLC
	ADC	#$01
	LDA	(helpind),Y
	BPL	norq
	RTS
norq
	BEQ	nh2
	BCC	showcom
	CPX	comprint
	BNE	nh
showcom
	JSR	$FFEE
	JMP	nh
nh2
	TYA
	PHA
	TXA
	PHA
	LDA	helptab,X
	STA	xtmp
	BEQ	nextcom
	BPL	go
	AND	#$7F
	STA	xtmp
	SEC
	LDA	helpind2
	SBC	xtmp
	STA	helpind2
	BCS	hover
	DEC	helpind2+1
hover
	LDA	xtmp
go
	TAY
	LDA	cpflag
	BEQ	cpok
	CPX	comprint
	BNE	skipcom
cpok
	LDA	#' '
	JSR	$FFEE
again
	DEY
	LDA	(helpind2),Y
	BPL	nh4
	TAX
	LDA	toktab-$80,X
	STA	htmp
	LDA	toktab-$81,X
	TAX
	JMP	nh5
nh3
	JSR	$FFEE
nh5
	LDA	tokens,X
	INX
	CPX	htmp
	BNE	nh3
nh4
	JSR	$FFEE
	CPY	#0
	BNE	again
skipcom
	CLC
	LDA	xtmp
	ADC	helpind2
	STA	helpind2
	BCC	hover2
	INC	helpind2+1
hover2
nextcom
	PLA
	TAX
	PLA
	TAY
	LDA	cpflag
	BEQ	docr
	CPX	comprint
	BNE	nocr
docr
	JSR	$FFE7
	JSR	indent2
nocr
	INX
	JMP	helploop

