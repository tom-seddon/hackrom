help
	LDA	($F2),Y
	CMP	#$0D
	BNE	spechelp
	JSR	$FFE7
	JSR	dispheadtitle
; Now print subtopics
	LDX	#0
topic2
	JSR	indent2
topic
	LDA	subjects,X
	BEQ	endtopic
	JSR	$FFEE
	INX
	BNE	topic
endtopic
	JSR	$FFE7
	INX
	LDA	subjects,X
	BPL	topic2
; Respond to specific *HELP
; eg *HELP MEM.
spechelp
	JSR	spechelp2
	JMP	romquit

spechelp2
